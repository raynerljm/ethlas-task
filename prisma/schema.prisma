// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Vote {
  id String @id @default(cuid())
  votedFor Int
  votedForName String
  votedAgainst Int
  votedAgainstName String
}

model Person {
  id Int @id 
  name String
  image String
}
